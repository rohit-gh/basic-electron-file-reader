{
  "name": "basic-electron-file-reader",
  "version": "1.0.0",
  "description": "A basic event based app for reading files",
  "author" : "rohit",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "electron index.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "CeoIU0aZqtHoeIxdsce",
    "directories" : {
      "output" : "dist"
    },
    "mac": {
      "category": "custom",
      "target" : "dmg"
    },
    "win" : {
      "target" : "msi"
    },
    "linux" : {
      "target" : "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^22.5.1"
  }
}
